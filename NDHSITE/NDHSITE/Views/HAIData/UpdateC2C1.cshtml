
@{
    ViewBag.Title = "UpdateC2C1";
}

<div class="page-header">
    <h3 class="page-title">Quản lý khách hàng cấp 1 vào cấp 2</h3>

    <ol class="breadcrumb">
        <li><a href="/home">Trang chủ</a></li>
        <li><a href="#">Quản lý khách hàng cấp 1 vào cấp 2</a></li>
    </ol>
</div> <!-- /.page-header -->

<div class="portlet portlet-boxed">

    <div class="portlet-body">
        <div class="layout layout-main-right layout-stack-sm">
            <div class="col-md-2 col-sm-3 layout-sidebar">

                <ul id="myTab" class="nav nav-layout-sidebar nav-stacked">
                    <li class="active">
                        <a href="#allstaff" data-toggle="tab">
                            Thêm
                        </a>
                    </li>
                    <li>
                        <a href="#addstaff_tab" data-toggle="tab">
                            Xóa
                        </a>
                    </li>
                </ul>

            </div> <!-- /.col -->

            <div class="col-md-10 col-sm-9 layout-main">
                <div id="settings-content" class="tab-content stacked-content">
                    <div class="tab-pane fade in active" id="allstaff">
                        <form method="post" enctype="multipart/form-data" onsubmit="if(!confirm('Cập nhật cấp 1 cho cấp 2?')){return false;}">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <input type="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" class="form-control" name="files" />
                                    </div>
                                    <button class="btn-primary btn" type="submit">Cập nhật</button>
                                </div>
                            </div>
                        </form>
                        <fieldset>
                            <table class="table table-striped table-bordered thumbnail-table">
                                <thead>
                                    
                                </thead>
                                <tbody></tbody>
                            </table>
                        </fieldset>
                    </div>
                    <div class="tab-pane fade" id="addstaff_tab">

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>