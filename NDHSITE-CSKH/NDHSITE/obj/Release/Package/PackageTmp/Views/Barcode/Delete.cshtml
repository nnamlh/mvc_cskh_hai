
@{
    ViewBag.Title = "Delete";
}

@if (ViewBag.MSG != null)
{
    <p>@ViewBag.MSG</p>
}

<form method="post" enctype="multipart/form-data" onsubmit="if(!confirm('Xoá thông tin barcode ?')){return false;}" action="/barcode/delete">
    <div class="row">

        <div class="col-sm-6">
            <div class="form-group">
                <label>Danh sách mã</label>
                <input type="file" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" class="form-control" name="files" required />
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-2">
            <div class="form-group">
                <button class="btn btn-primary" type="submit">Xóa</button>
            </div>
        </div>
    </div>
</form>




